//Find Second Largest
//Problem Code: SECLAR


#include <iostream>
using namespace std;

int main() {
	int a,b,c,x,y;
	cin>>a>>b>>c;
	x=a;
	if((a>b && b>c) || ( c>b && b>a))
	    cout<<b;
    else if((b>a && a>c) || (c>a && a>b))
        cout<<a;
    else if((a>c && c>b )||(b>c && c>a ))
        cout<<c;
	return 0;
}
